<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.hbhk">
	<!-- 测试 -->
	<resultMap type="org.hbhk.aili.core.share.domain.UserEntity" id="UserMap">
		<id column="username" property="username" jdbcType="VARCHAR" />
		<id column="password" property="password" jdbcType="VARCHAR" />
	</resultMap>
         
     <insert id="insertUser" parameterType="org.hbhk.aili.core.share.domain.UserEntity">
       insert into  t_hbhk_user  values(#{username},#{password});
     </insert>    
       
</mapper>